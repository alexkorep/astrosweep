; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "StateMachine",
"class": "PlayerStateMachine",
"language": "GDScript",
"path": "res://scenes/player/player_state_machine.gd"
}, {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://scripts/state_machine/state.gd"
}, {
"base": "Node",
"class": "StateMachine",
"language": "GDScript",
"path": "res://scripts/state_machine/state_machine.gd"
} ]
_global_script_class_icons={
"PlayerStateMachine": "",
"State": "",
"StateMachine": ""
}

[application]

config/name="AstroSweep"
run/main_scene="res://scenes/main_screen/main_screen.tscn"
config/icon="res://icon.png"

[autoload]

GameState="*res://scripts/game_state.gd"
ShipModels="*res://scripts/ship_models.gd"

[display]

window/size/width=360
window/size/height=720
window/size/resizable=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
